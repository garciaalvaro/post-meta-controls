		"copyfiles": "^2.4.1",
		"css-loader": "^5.0.1",
		"css-minimizer-webpack-plugin": "^1.2.0",
		"dompurify": "^2.2.6",
		"immer": "^8.0.1",
		"mini-css-extract-plugin": "^1.3.4",
		"moment": "^2.29.1",
		"nib": "^1.1.2",
		"react": "^16.14.0",
		"react-dates": "^21.8.0",
		"react-dom": "^16.14.0",
		"react-sortable-hoc": "^1.11.0",
		"react-with-direction": "^1.3.1",
		"replace-in-file": "^6.1.0",
		"stylus": "^0.54.8",
