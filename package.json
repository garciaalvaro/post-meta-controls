{
	"name": "post-meta-controls",
	"description": "Post Meta Controls",
	"version": "1.3.4",
	"homepage": "https://wordpress.org/plugins/post-meta-controls/",
	"author": "Alvaro Garcia",
	"private": true,
	"scripts": {
		"moment": "webpack --mode production --config ./webpack/moment.config.babel.js",
		"dev": "webpack --mode development --watch --config ./webpack/build.config.babel.js",
		"copy": "webpack --mode production --config ./webpack/copy.config.babel.js && rm _release/_temp.js",
		"version": "webpack --mode production --config ./webpack/version.config.babel.js && rm _temp.js",
		"pragma": "webpack --mode production --config ./webpack/pragma.config.babel.js && rm _release/_temp.js",
		"clean": "rm -rf build _release",
		"prebuild": "npm run test && npm run clean && npm run version",
		"build": "webpack --mode production --config ./webpack/build.config.babel.js",
		"postbuild": "npm run moment && npm run copy && npm run pragma",
		"test": "npm run test:types && npm run test:lint && npm run test:prettier",
		"test:types": "tsc",
		"test:lint": "eslint \"**/*.{js,ts,tsx}\"",
		"test:prettier": "prettier --check \"**/*.{js,ts,tsx,php,json}\"",
		"prettier": "prettier --write \"**/*.{js,ts,tsx,php,json}\""
	},
