{
	"compilerOptions": {
		"target": "ESNext",
		"moduleResolution": "Node",
		"noEmit": true,
		"strict": true,
		"esModuleInterop": true,
		"jsx": "react",
		"baseUrl": ".",
		"paths": {
			"Components/*": ["src/components/*"],
			"store/*": ["src/store/*"],
			"utils/*": ["src/utils/*"]
		}
	},
	"include": ["src", "types"]
}
