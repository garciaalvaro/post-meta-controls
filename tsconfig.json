{
	"compilerOptions": {
		"target": "ESNext",
		"moduleResolution": "Node",
		"noEmit": true,
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"allowJs": true,
		"jsx": "react",
		"baseUrl": ".",
		"paths": {
			"@/*": ["src/*"]
		}
	},
	"include": ["src", "types"],
	"exclude": ["_extras", "_release", "dist", "node_modules", "pro"]
}
